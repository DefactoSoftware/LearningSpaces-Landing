---
i18n_page_name: index
---

<%= partial "header" %>

<div id="main" role="main">
<section id="intro">
  <h1>
    <%= t("healthcare.head_title") %>
  </h1>

  <div class="bottom">
    <%= link_to t("trial.request"), "trial", class: "button button-orange button-big" %>
  </div>
</section>

<!-- Features -->

  <%= partial "features-healthcare" %>

<section id="announcement">
  <div class="container">

    <div class="quote">
      <div class="avatar" style="background-image: url(http://photos-b.ak.instagram.com/hphotos-ak-xpf1/10483442_1426344384313553_589866955_n.jpg)"></div>

      <div class="bubble">
        <p><%= t("healthcare.quote_heading_html") %></p>
        <small><%= t("healthcare.quote_html") %></small>
      </div>
    </div>

  </div>
</section>

<!-- Screenshots -->

<section id="screenshots">
  <div class="container">

    <div class="browser scroll">
      <div class="ui">
        <div class="ooo"></div>
        <div class="www">www.learningspaces.io</div>
      </div>
      <div class="window">
        <div class="document">
          <div class="scroll">
            <div class="scroll-inner">
              <%= image_tag("screenshot/spaces.png", class: "options") %>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>

<section id="pricing">
  <div class="container">

    <div class="section-head">
      <h2><%= t("healthcare.story_head") %></h2>
      <hr>
    </div>

    <p class="pricing-text">
      <%= t("healthcare.story_html") %>
    </p>


    <p>
     <%= image_tag("screenshot/paths.png", class: "") %>
    </p>

    <br>
    <%= link_to t("trial.request"), "trial", class: "button button-orange" %>

  </div>
</section>


  <%= partial "contact" %>
</div>

<%= partial "footer" %>
